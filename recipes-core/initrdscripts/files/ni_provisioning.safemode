#!/bin/bash
set -e

source /ni_provisioning.safemode.common

install_safemode
install_bootmode_file
install_grubenv
set_versions

fixup_configfs

echo $LOG_LEVEL > /proc/sys/kernel/printk
sanity_check

print_info "Re-enabling automount..."
enable_automount
print_done

trap - ERR
exec 1>&3
exec 2>&4

provision_successfull
