 libcap/cap_file.c | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/libcap/cap_file.c b/libcap/cap_file.c
index 40756ea..76aac8c 100644
--- a/libcap/cap_file.c
+++ b/libcap/cap_file.c
@@ -13,7 +13,7 @@
 /*
